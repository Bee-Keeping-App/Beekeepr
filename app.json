{
  "expo": {
    "name": "Beekeepr",
    "slug": "Beekeepr",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "scheme": "beekeepr",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.beekeepr",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "Beekeepr uses your location to show local weather and radar."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.beekeepr",
      "permissions": ["ACCESS_COARSE_LOCATION", "ACCESS_FINE_LOCATION"]
    },
    "plugins": [
      "expo-asset",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#ffffff",
          "image": "./assets/splash-icon.png"
        }
      ],
      [
        "expo-location",
        {
          "locationWhenInUsePermission": "Allow Beekeepr to access your location to show local weather and radar."
        }
      ]
    ]
  }
}
